{
  "name": "@tscircuit/file-server",
  "main": "./dist/bundle.js",
  "type": "module",
  "version": "0.0.34",
  "bin": {
    "file-server": "./dist/cli.js"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/bun": "latest",
    "@types/react": "18.3.4",
    "next": "^14.2.5",
    "redaxios": "^0.5.1",
    "tsup": "^8.3.5",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "typescript": "^5.0.0",
    "zod": "*"
  },
  "dependencies": {
    "winterspec": "^0.0.86",
    "zustand": "^4.5.5",
    "zustand-hoist": "^2.0.1"
  },
  "scripts": {
    "start": "bun run dev",
    "dev": "winterspec dev -p 3062",
    "build": "bun run build:bundle && bun run build:cli",
    "build:bundle": "winterspec bundle -o dist/bundle.js",
    "build:cli": "tsup cli.ts --format esm --dts",
    "next:dev": "next dev",
    "format": "biome format --write .",
    "format:check": "biome format ."
  }
}
